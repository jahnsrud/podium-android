<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".LibraryFragment">
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <Button
                android:text="Library"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/button"/>
        <Button
                android:text="OPEN_PODCAST_TEST"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/podcastButton"/>
        <fragment
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:name="no.jahnsrud.podium.PodcastListFragment"
                android:id="@+id/fragment"/>
    </LinearLayout>
</FrameLayout>